<div>
    <form id="userEditForm" class="form-horizontal container-fluid">
        <div class="row">
            <div class="form-group col-md-6">
                <label for="userEditFormId" class="col-sm-4 control-label" data-bind="text: $root.labels.id"></label>
                <div class="col-sm-8">
                    <input id="userEditFormId" type="text" class="form-control" data-bind="value: id" readonly />
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="userEditFormName" class="col-sm-4 control-label" data-bind="text: $root.labels.name"></label>
                <div class="col-sm-8">
                    <input id="userEditFormName" type="text" class="form-control" data-bind="value: name" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="userEditFormAccount" class="col-sm-4 control-label" data-bind="text: $root.labels.username"></label>
                <div class="col-sm-8">
                    <input id="userEditFormAccount" type="text" class="form-control" data-bind="value: username" />
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="userEditFormCmsText" class="col-sm-4 control-label" data-bind="text: $root.labels.password"></label>
                <div class="col-sm-8">
                    <input id="userEditFormCmsText" type="text" class="form-control" data-bind="value: password" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="userEditFormSalary" class="col-sm-4 control-label" data-bind="text: $root.labels.role"></label>
                <div class="col-sm-8">
                    <input id="userEditFormSalary" type="text" class="form-control" data-bind="value: role" />
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="userEditFormSalary" class="col-sm-4 control-label" data-bind="text: $root.labels.location"></label>
                <div class="col-sm-8">
                    <input id="userEditFormSalary" type="text" class="form-control" data-bind="value: location" />
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="userEditFormSalary" class="col-sm-4 control-label" data-bind="text: $root.labels.email"></label>
                <div class="col-sm-8">
                    <input id="userEditFormSalary" type="text" class="form-control" data-bind="value: email" />
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="userEditFormSalary" class="col-sm-4 control-label" data-bind="text: $root.labels.active"></label>
                <div class="col-sm-8">
                    <input id="userEditFormSalary" type="text" class="form-control" data-bind="value: active" />
                </div>
            </div>
        </div>
    </form>
</div>